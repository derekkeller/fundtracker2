<h2>Choose a month to view:</h2>

<table>
	<tr>
		<th>Date</th>
	</tr>
	<% @reports.each do |report| %>
	<tr>
		<td><%= link_to report.period, organization_fund_company_report_path(@organization, @fund, @company, report) %></td>
	</tr>
	<% end %>
</table>

<div class="back_footer">
	<%= link_to "Add Report", new_organization_fund_company_report_path(@organization, @fund, @company) %>
</div>