<h2>Financials Show (single company)</h2>

<table>
	<%= render @header_row_partial %>
	<tr>
		<td>Bookings</td>
		<% @financial_summary.financial_set.each do |fs| %>
			<td><%= nh(fs.bookings) %></td>
		<% end %>
	</tr>
	<tr>
		<td>Revenue</td>
		<% @financial_summary.financial_set.each do |fs| %>
			<td><%= nh(fs.revenue) %></td>
		<% end %>
	</tr>
	<tr>
		<td>COGS</td>
		<% @financial_summary.financial_set.each do |fs| %>
			<td><%= nh(fs.cogs) %></td>
		<% end %>
	</tr>
	<tr>
		<td>Operating Expenses</td>
		<% @financial_summary.financial_set.each do |fs| %>
			<td><%= nh(fs.operating_expenses) %></td>
		<% end %>
	</tr>
</table>

<p><%= Rails.logger.info("* * #{params.inspect}") %></p>

<div class='back_footer'>
	<%= link_to 'Add Financial', new_organization_fund_company_financial_path(@organization, @fund, @company) %> |
	<%= link_to 'Back', organization_fund_company_path(@organization, @fund, @company) %>
</div>